{
  "name": "@folio/tags",
  "version": "7.0.0",
  "description": "Tags manager",
  "repository": "folio-org/ui-tags",
  "publishConfig": {
    "registry": "https://repository.folio.org/repository/npm-folio/"
  },
  "license": "Apache-2.0",
  "engines": {
    "node": ">=14.0.0"
  },
  "main": "index.js",
  "stripes": {
    "actsAs": [
      "settings"
    ],
    "displayName": "ui-tags.meta.title",
    "route": "/tags",
    "hasSettings": true,
    "queryResource": "query",
    "icons": [
      {
        "name": "app",
        "alt": "",
        "title": "Tags"
      }
    ],
    "okapiInterfaces": {
      "tags": "1.0"
    },
    "permissionSets": [
      {
        "permissionName": "module.tags.enabled",
        "displayName": "UI: Tags module is enabled"
      },
      {
        "permissionName": "settings.tags.enabled",
        "displayName": "Settings (Tags): display list of settings pages",
        "subPermissions": [
          "settings.enabled"
        ],
        "visible": false
      },
      {
        "permissionName": "ui-tags.settings.view",
        "displayName": "Settings (Tags): Can view tags settings",
        "subPermissions": [
          "settings.tags.enabled",
          "configuration.entries.collection.get"
        ],
        "visible": true
      },
      {
        "permissionName": "ui-tags.settings.all",
        "displayName": "Settings (Tags): Can enable or disable tags for all apps",
        "subPermissions": [
          "ui-tags.settings.view",
          "configuration.entries.collection.put",
          "configuration.entries.collection.post"
        ],
        "visible": true
      },
      {
        "permissionName": "ui-tags.all",
        "displayName": "Tags: All permissions",
        "subPermissions": [
          "module.tags.enabled",
          "ui-tags.view",
          "ui-tags.create",
          "ui-tags.edit",
          "ui-tags.delete"
        ],
        "visible": true
      },
      {
        "permissionName": "ui-tags.view",
        "subPermissions": [
          "module.tags.enabled",
          "tags.collection.get"
        ],
        "visible": true
      },
      {
        "permissionName": "ui-tags.create",
        "subPermissions": [
          "module.tags.enabled",
          "tags.collection.get",
          "tags.item.post"
        ],
        "visible": false
      },
      {
        "permissionName": "ui-tags.edit",
        "subPermissions": [
          "module.tags.enabled",
          "tags.collection.get",
          "tags.item.get",
          "tags.item.put"
        ],
        "visible": false
      },
      {
        "permissionName": "ui-tags.delete",
        "subPermissions": [
          "module.tags.enabled",
          "tags.collection.get",
          "tags.item.get",
          "tags.item.delete"
        ],
        "visible": false
      }
    ]
  },
  "scripts": {
    "start": "yarn stripes serve",
    "lint": "eslint .",
    "test": "jest --coverage --verbose",
    "build-mod-descriptor": "stripes mod descriptor --full --strict | jq '.[]' > module-descriptor.json ",
    "formatjs-compile": "formatjs compile-folder --ast --format simple ./translations/ui-tags ./translations/ui-tags/compiled"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.17.0",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@folio/eslint-config-stripes": "^6.1.0",
    "@folio/stripes": "^8.0.0",
    "@folio/stripes-cli": "^2.6.0",
    "@folio/stripes-components": "^11.0.0",
    "@folio/stripes-core": "^9.0.0",
    "@folio/stripes-form": "^8.0.0",
    "@folio/stripes-smart-components": "^8.0.0",
    "@formatjs/cli": "^4.2.6",
    "@testing-library/dom": "^8.19.0",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^12.1.5",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.32.0",
    "history": "^5.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-environment-jsdom": "^26.6.0",
    "jest-junit": "^12.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-intl": "^5.8.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.0",
    "regenerator-runtime": "^0.13.10"
  },
  "dependencies": {
    "prop-types": "^15.5.10",
    "redux-form": "^8.3.7"
  },
  "peerDependencies": {
    "@folio/stripes": "^8.0.0",
    "react": "^17.0.2",
    "react-intl": "^5.8.0",
    "react-redux": "^8.0.5"
  }
}
