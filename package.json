{
  "name": "@folio/tags",
  "version": "6.2.0",
  "description": "Tags manager",
  "repository": "folio-org/ui-tags",
  "publishConfig": {
    "registry": "https://repository.folio.org/repository/npm-folio/"
  },
  "license": "Apache-2.0",
  "engines": {
    "node": ">=14.0.0"
  },
  "main": "index.js",
  "stripes": {
    "actsAs": [
      "settings"
    ],
    "displayName": "ui-tags.meta.title",
    "route": "/tags",
    "hasSettings": true,
    "queryResource": "query",
    "icons": [
      {
        "name": "app",
        "alt": "",
        "title": "Tags"
      }
    ],
    "okapiInterfaces": {
      "tags": "1.0"
    },
    "permissionSets": [
      {
        "permissionName": "module.tags.enabled",
        "displayName": "UI: Tags module is enabled"
      },
      {
        "permissionName": "settings.tags.enabled",
        "displayName": "Settings (Tags): display list of settings pages",
        "subPermissions": [
          "settings.enabled"
        ],
        "visible": false
      },
      {
        "permissionName": "ui-tags.settings",
        "displayName": "Settings (Tags): Can enable or disable tags for all apps",
        "subPermissions": [
          "settings.tags.enabled"
        ],
        "visible": true
      },
      {
        "permissionName": "ui-tags.all",
        "displayName": "Tags: All permissions",
        "subPermissions": [
          "module.tags.enabled",
          "ui-tags.view",
          "ui-tags.create",
          "ui-tags.edit",
          "ui-tags.delete"
        ],
        "visible": true
      },
      {
        "permissionName": "ui-tags.view",
        "subPermissions": [
          "module.tags.enabled",
          "tags.collection.get"
        ],
        "visible": true
      },
      {
        "permissionName": "ui-tags.create",
        "subPermissions": [
          "module.tags.enabled",
          "tags.collection.get",
          "tags.item.post"
        ],
        "visible": false
      },
      {
        "permissionName": "ui-tags.edit",
        "subPermissions": [
          "module.tags.enabled",
          "tags.collection.get",
          "tags.item.get",
          "tags.item.put"
        ],
        "visible": false
      },
      {
        "permissionName": "ui-tags.delete",
        "subPermissions": [
          "module.tags.enabled",
          "tags.collection.get",
          "tags.item.get",
          "tags.item.delete"
        ],
        "visible": false
      }
    ]
  },
  "scripts": {
    "start": "yarn stripes serve",
    "lint": "eslint .",
    "test": "yarn run test:jest",
    "test:jest": "jest --coverage --colors",
    "build-mod-descriptor": "stripes mod descriptor --full --strict | jq '.[]' > module-descriptor.json ",
    "formatjs-compile": "formatjs compile-folder --ast --format simple ./translations/ui-tags ./translations/ui-tags/compiled"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.17.0",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@folio/eslint-config-stripes": "^6.1.0",
    "@folio/stripes": "^7.0.0",
    "@folio/stripes-cli": "^2.6.100000177",
    "@formatjs/cli": "^4.2.6",
    "@testing-library/dom": "^8.19.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.5",
    "babel-jest": "^29.2.2",
    "eslint": "^7.32.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.2.2",
    "jest-environment-jsdom": "^29.2.2",
    "jest-junit": "^14.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-intl": "^5.8.0",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.0"
  },
  "dependencies": {
    "prop-types": "^15.5.10",
    "redux-form": "^8.3.7",
    "remove": "^0.1.5"
  },
  "peerDependencies": {
    "@folio/stripes": "^7.0.0",
    "react": "^17.0.2",
    "react-intl": "^5.8.0"
  }
}
