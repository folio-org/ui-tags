{
  "name": "@folio/tags",
  "version": "9.0.0",
  "description": "Tags manager",
  "repository": "folio-org/ui-tags",
  "publishConfig": {
    "registry": "https://repository.folio.org/repository/npm-folio/"
  },
  "license": "Apache-2.0",
  "main": "index.js",
  "stripes": {
    "actsAs": [
      "settings"
    ],
    "displayName": "ui-tags.meta.title",
    "route": "/tags",
    "hasSettings": true,
    "queryResource": "query",
    "icons": [
      {
        "name": "app",
        "alt": "",
        "title": "Tags"
      }
    ],
    "okapiInterfaces": {
      "tags": "1.0"
    },
    "permissionSets": [
      {
        "permissionName": "module.tags.enabled",
        "displayName": "UI: Tags module is enabled"
      },
      {
        "permissionName": "settings.tags.enabled",
        "displayName": "Settings (Tags): display list of settings pages",
        "subPermissions": [
          "settings.enabled"
        ],
        "visible": false
      },
      {
        "permissionName": "ui-tags.settings.view",
        "displayName": "Settings (Tags): Can view tags settings",
        "subPermissions": [
          "settings.tags.enabled",
          "mod-settings.entries.collection.get",
          "mod-settings.entries.item.get",
          "mod-settings.global.read.ui-tags.tags.manage",
          "configuration.entries.collection.get"
        ],
        "visible": true
      },
      {
        "permissionName": "ui-tags.settings.all",
        "displayName": "Settings (Tags): Can enable or disable tags for all apps",
        "subPermissions": [
          "ui-tags.settings.view",
          "mod-settings.entries.item.post",
          "mod-settings.entries.item.put",
          "mod-settings.global.write.ui-tags.tags.manage",
          "configuration.entries.item.put",
          "configuration.entries.item.post"
        ],
        "visible": true
      },
      {
        "permissionName": "ui-tags.all",
        "displayName": "Tags: All permissions",
        "subPermissions": [
          "module.tags.enabled",
          "ui-tags.view",
          "ui-tags.create",
          "ui-tags.edit",
          "ui-tags.delete"
        ],
        "visible": true
      },
      {
        "permissionName": "ui-tags.view",
        "displayName": "Tags on records: View only",
        "subPermissions": [
          "module.tags.enabled",
          "tags.collection.get"
        ],
        "visible": true
      },
      {
        "permissionName": "ui-tags.create",
        "subPermissions": [
          "module.tags.enabled",
          "tags.collection.get",
          "tags.item.post"
        ],
        "visible": false
      },
      {
        "permissionName": "ui-tags.edit",
        "subPermissions": [
          "module.tags.enabled",
          "tags.collection.get",
          "tags.item.get",
          "tags.item.put"
        ],
        "visible": false
      },
      {
        "permissionName": "ui-tags.delete",
        "subPermissions": [
          "module.tags.enabled",
          "tags.collection.get",
          "tags.item.get",
          "tags.item.delete"
        ],
        "visible": false
      },
      {
        "permissionName": "mod-settings.global.read.ui-tags.tags.manage",
        "displayName": "Settings (Tags): Read the tag settings",
        "visible": false
      },
      {
        "permissionName": "mod-settings.global.write.ui-tags.tags.manage",
        "displayName": "Settings (Tags): Write the tag settings",
        "visible": false
      }
    ]
  },
  "scripts": {
    "start": "yarn stripes serve",
    "lint": "eslint .",
    "test": "jest --coverage --verbose",
    "build-mod-descriptor": "stripes mod descriptor --full --strict | jq '.[]' > module-descriptor.json ",
    "formatjs-compile": "stripes translate compile"
  },
  "devDependencies": {
    "@folio/eslint-config-stripes": "^8.0.0",
    "@folio/jest-config-stripes": "^3.0.0",
    "@folio/stripes": "^10.0.0",
    "@folio/stripes-cli": "^4.0.0",
    "@folio/stripes-components": "^13.0.0",
    "@folio/stripes-core": "^11.0.0",
    "@folio/stripes-form": "^10.0.0",
    "@folio/stripes-smart-components": "^10.0.0",
    "eslint": "^7.32.0",
    "history": "^5.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-intl": "^7.1.5",
    "react-redux": "^8.0.5",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.0",
    "regenerator-runtime": "^0.13.10"
  },
  "dependencies": {
    "prop-types": "^15.5.10",
    "redux-form": "^8.3.7"
  },
  "peerDependencies": {
    "@folio/stripes": "^10.0.0",
    "react": "^18.2.0",
    "react-intl": "^7.1.5",
    "react-redux": "^8.0.5"
  }
}
